{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "prefer-const": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      false
    ]
  },
  "rulesDirectory": []
}